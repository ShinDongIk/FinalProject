		<mapper resource="/mappers/ratePlan/rateplan-mapper.xml"/>
		<mapper resource="/mappers/memberModal/memberModal-mapper.xml"/>
		<mapper resource="/mappers/user/user-mapper.xml"/>
		<mapper resource="/mappers/board/board-mapper.xml"/>
		<mapper resource="/mappers/inquiry/inquiry-mapper.xml"/>
		<mapper resource="/mappers/reply/reply-mapper.xml"/>
		<mapper resource="/mappers/memManage/memManage-mapper.xml"/>
		<mapper resource="/mappers/party/party-mapper.xml"/>
